<section id="contact">
    <div class="container">
        <h2>User Story</h2>
        <hr class="sep">
        <p>Edit Your Story</p>
        <div class="col-md-6 col-md-offset-3 wow fadeInUp" data-wow-delay=".3s">
        <%= console.log("$$$$$$$" + story) %>
        <%= console.log("***************" + story.id) %>

            <form method="POST" action="/profile/edit/<%= story.id %>">
                <div class="form-group">
                    <label for="Title">Title</label>
                    <input type="text" name="title" class="form-control" id="Title" value="<%= story.title %>">
                </div>
                <div class="form-group">
                    <textarea name="given" class="form-control" rows="1" ><%= story.given %></textarea>
                </div>
                <div class="form-group">
                    <textarea name="when" class="form-control" rows="1" ><%= story.when %></textarea>
                </div>
                <div class="form-group">
                    <textarea name="and" class="form-control" rows="1" ><%= story.and  %></textarea>
                </div>
                <div class="form-group">
                    <textarea name="then" class="form-control" rows="1" ><%= story.then %></textarea>
                </div>
                <input class="btn-block" type="submit" value="Update Story" >
            </form>
        </div>
    </div>
</section>
